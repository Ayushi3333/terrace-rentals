<div class="container w-50">
    <%= simple_form_for(@terrace) do |t| %>
    <%= t.input :description %>
    <%= t.input :size %>
    <%= t.input :floor %>
    <%= t.input :price %>
    <%= t.input :address %>
    <%= t.input :photos, as: :file, input_html: { multiple: true } %>
    <%= t.submit "Add your terrace", class:"btn btn-flat center-button mb-3" %>
  <% end %>
</div>